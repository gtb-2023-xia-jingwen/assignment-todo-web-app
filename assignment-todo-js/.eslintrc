{
  "env": {
    "es2021": true,
    "browser": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:cypress/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "ignorePatterns": [
    "dist/*"
  ]
}
